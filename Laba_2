import math
a1=float(input("a1="))
b1=float(input("b1="))
c1=float(input("c1="))
a2=float(input("a2="))
b2=float(input("b2="))
c2=float(input("c2="))
P1=(a1+b1+c1)/2
S1=math.sqrt(P1*(P1-a1)*(P1-b1)*(P1-c1))
P2=(a2+b2+c2)/2
S2=math.sqrt(P2*(P2-a2)*(P2-b2)*(P2-c2))
if S1>S2:
	print('max=',round(S1,2))
else:
	print('max=',round(S2,2))

