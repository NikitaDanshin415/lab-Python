import math
x=int(input("C1="))
y=int(input("C2="))
q=int(input("C1*C2="))
l = x*y
if (x>-10 and x<10) and (y>-10 and y<10) and (x*y == q):
	print('verno')
else:
	print('ne verno pravilniy otvet = ',x*y)
  
  
  
  
  
  
  
  
  
  
