 import math

N1=int(input("N1="))
N2=int(input("N2="))
i=0;
if (N1==0) and (N2==0):
  print('Input error')
else:
  while N1!=N2:
    if N1>N2:
      while N1>N2:     #НОК
        N1=N1-N2
    else:
      while N2>N1:
        N2=N2-N1
print('nok=',N2)
 ///////////////////////////////////
 p=1
n=0
while n>=0:
	n = int(input())
	if n>0:
		p=p*n         #ввод чисел до 1 отрицат числа и их произведение
print(p)
 ///////////////////////////////////
 import math
s=int(input("s="))
x=int(input("x="))
n=1
q=0                           
for i in range (1,s):
	q=((x**n)*(math.sin(n)*(math.pi/4))) + q
print('sum=',round(q,2))
